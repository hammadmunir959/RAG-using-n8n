# Production Environment Variables
# Copy this to .env and fill in your values

# n8n Configuration
N8N_BASE_URL=http://n8n:5678
N8N_UPLOAD_WEBHOOK_ID=your-upload-webhook-id
N8N_CHAT_WEBHOOK_ID=your-chat-webhook-id
N8N_CHAT_WEBHOOK_PATH=webhook/your-chat-path/chat
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=CHANGE_ME_STRONG_PASSWORD

# AI Configuration (Required)
GROQ_API_KEY=gsk_your_groq_api_key_here
SCRAPER_ANT_API_KEY=your_scrapingant_key_here

# Model Settings
LLM_MODEL=llama-3.3-70b-versatile
EMBEDDING_MODEL=all-MiniLM-L6-v2

# Database (SQLite for simple deploy, PostgreSQL for scale)
DATABASE_URL=sqlite:///./data/app.db
# DATABASE_URL=postgresql://user:password@db:5432/docint
